<h1>Listing educations</h1>

<table>
  <tr>
    <th>User</th>
    <th>University</th>
    <th>Faculty name</th>
    <th>Enrollment date</th>
    <th>Graduation date</th>
    <th>Domain</th>
    <th>Specialization</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @educations.each do |education| %>
  <tr>
    <td><%= education.user_id %></td>
    <td><%= education.university %></td>
    <td><%= education.faculty_name %></td>
    <td><%= education.enrollment_date %></td>
    <td><%= education.graduation_date %></td>
    <td><%= education.domain %></td>
    <td><%= education.specialization %></td>
    <td><%= link_to 'Show', education %></td>
    <td><%= link_to 'Edit', edit_education_path(education) %></td>
    <td><%= link_to 'Destroy', education, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Education', new_education_path %>
